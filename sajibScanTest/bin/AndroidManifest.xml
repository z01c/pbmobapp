<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.lilait.football.toptenteams"
      android:versionCode="1"
      android:versionName="1.1">
         <uses-sdk android:minSdkVersion="8" />
             <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
     <uses-permission android:name="android.permission.SET_WALLPAPER"/>
   <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application android:label="@string/app_name"
        android:icon="@drawable/moon2">
        <activity android:name="MyAndroidAppActivity"
                  android:label="@string/app_name"
                  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lilait.football.toptenteams.CitiesActivity"></activity>
        <activity android:name="com.lilait.football.toptenteams.CitiesActivity1"></activity>
        <activity android:name="com.lilait.football.toptenteams.SecondActivityGroup"></activity>
        <activity android:name="com.lilait.football.toptenteams.FirstGroup"></activity>
        <activity android:name="com.lilait.football.toptenteams.FirstGroup1"></activity>
        <activity android:name="com.lilait.football.toptenteams.ShowCity"
            android:screenOrientation="portrait"></activity>
        <service android:enabled="true" android:name="com.sound.kd.MusicService"></service>
           <activity android:name="com.google.ads.AdActivity"
        android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
        android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"  />
  <activity
            android:name="com.google.zxing.client.android.CaptureActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="Scan Barcode"
            android:screenOrientation="sensorLandscape"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:windowSoftInputMode="stateAlwaysHidden" >
            <intent-filter>
                <action android:name="com.google.zxing.client.android.SCAN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
   
    </application>
</manifest> 
